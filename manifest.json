{
"manifest_version": 2,
    "name": "Rosie",
    "description": "Undermining Language Detector",
    "version": "1.0",
	"browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}